{
  "name": "app",
  "private": true,
  "version": "1.0.0",
  "description": "Pick some lunch",
  "license": "MIT",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "yarn prebuild && nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "yarn prebuild && nest start",
    "start:dev": "yarn prebuild && nest start --watch",
    "start:debug": "yarn prebuild && nest start --debug --watch",
    "start:prod": "yarn prebuild && node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json"
  },
  "dependencies": {
    "@zeit/next-css": "^1.0.1",
    "antd": "^3.26.12",
    "ky": "^0.17.0",
    "query-string": "^6.11.0",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "swr": "^0.1.18"
  },
  "devDependencies": {
    "@types/react": "^16.9.21",
    "babel-plugin-import": "^1.13.0",
    "null-loader": "^3.0.0"
  }
}
