{
  "name": "server",
  "private": true,
  "version": "1.0.0",
  "description": "Pick some lunch",
  "license": "MIT",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "yarn prebuild && nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "yarn prebuild && nest start",
    "start:dev": "nest start --watch",
    "start:debug": "yarn prebuild && nest start --debug --watch",
    "start:prod": "yarn prebuild && node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json"
  },
  "dependencies": {
    "@nestjs/common": "^6.10.14",
    "@nestjs/config": "^0.2.3",
    "@nestjs/core": "^6.10.14",
    "@nestjs/platform-fastify": "^6.11.8",
    "antd": "^3.26.12",
    "ioredis": "^4.16.0",
    "ky": "^0.17.0",
    "ky-universal": "^0.5.0",
    "pg": "^7.18.2",
    "pg-promise": "^10.4.3",
    "query-string": "^6.11.0",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.0",
    "rxjs": "^6.5.4",
    "uuid": "^3.4.0",
    "winston": "^3.2.1"
  },
  "devDependencies": {
    "@nestjs/cli": "^6.13.2",
    "@nestjs/schematics": "^6.8.1",
    "@nestjs/testing": "^6.10.14",
    "@types/ioredis": "^4.14.8",
    "@types/jest": "^24.0.25",
    "@types/node": "^13.1.6",
    "@types/pg": "^7.14.1",
    "@types/pg-promise": "^5.4.3",
    "@types/supertest": "^2.0.8",
    "@types/uuid": "^3.4.7",
    "@typescript-eslint/eslint-plugin": "^2.12.0",
    "@typescript-eslint/parser": "^2.12.0",
    "babel-plugin-import": "^1.13.0",
    "babel-plugin-transform-imports": "^2.0.0",
    "jest": "^24.9.0",
    "supertest": "^4.0.2",
    "ts-jest": "^24.3.0",
    "ts-loader": "^6.2.1",
    "ts-node": "^8.6.0",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^3.7.4"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
